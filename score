//Baskets/jumps * 100
public class Score {
  private int collisions = collisions();
  private int jumps = jumpCount();

  private int findScore() {
    if (jumps > 0) {
      return collisions / jumps * 100;
    } else { 
      return 0;
    }
  }


  public void showScore() {
    String score = str(findScore());
    println(score);
  }
}
